<project name="Deployment Scripts" default="Jitendra.zaa.retrieve" basedir="." xmlns:sf="antlib:com.salesforce">
 	   
    <property file="build.properties"/>
    <property environment="env"/> 
	<property name="test.Level" value="NoTestRun"/>
	<property name="test.Level.LocalRun" value="RunLocalTests"/>
	 
	 <target name="Jitendra.zaa.retrieve">
		<echo message="${sfdc.Jitendra.zaa.retrieveMessage}"/>
		<echo message="${sfdc.Jitendra.zaa.username}"/>
		<echo message="folder - ${root.dir}/${sfdc.Jitendra.zaa.retrieveTarget}"/>
		<echo message="path - ${root.dir}/${sfdc.Jitendra.zaa.unpackaged}"/>
		
		<sf:retrieve 
			username="${sfdc.Jitendra.zaa.username}"
			password="${sfdc.Jitendra.zaa.password}" 
			serverurl="${sfPRO.serverurl}"
			retrieveTarget="${root.dir}/${sfdc.Jitendra.zaa.retrieveTarget}"
			unpackaged="${root.dir}/${sfdc.Jitendra.zaa.unpackaged}"
			/> 
	</target> 
	
</project>